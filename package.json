{
  "name": "oh-hey-frontend",
  "version": "1.0.0",
  "private": true,
  "description": "Проект справочного сайта с материалами по веб-разработке",
  "scripts": {
    "test": "yarn run test:editor && yarn run test:pug && yarn run test:style",
    "test:editor": "editorconfig-cli",
    "test:pug": "glob-exec \"source/**/*.pug\" -- \"pug-lint {{files.join(' ')}}\"",
    "test:style": "stylelint \"source/less/**/*.less\" --syntax=less",
    "test:js": "eslint source/js/**/*.js",
    "start": "gulp",
    "build": "gulp build",
    "lec": "gulp correct-line-ending",
    "lint-staged": "lint-staged"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/less/**/*.less"
  ],
  "lint-staged": {
    "*.{js,pug,md}": "editorconfig-cli",
    "*.pug": "pug-lint",
    "*.less": "stylelint --syntax=less",
    "*.js": "eslint"
  },
  "devDependencies": {
    "@babel/core": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "@barba/core": "^2.9.7",
    "@htmlacademy/editorconfig-cli": "1.x",
    "ajv": "^6.12.2",
    "autoprefixer": "9.8.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.24.7",
    "del": "^5.1.0",
    "eslint": "^7.1.0",
    "gh-pages": "^3.0.0",
    "glob-exec": "^0.1.1",
    "gulp": "^4.0.2",
    "gulp-csso": "^4.0.1",
    "gulp-html-prettify": "^0.0.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-is-binary": "^0.1.2",
    "gulp-less": "^4.0.1",
    "gulp-line-ending-corrector": "^1.0.3",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "8.0.0",
    "gulp-pug": "^4.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-uglify": "^3.0.0",
    "husky": "^4.2.5",
    "less": "^3.11.1",
    "lint-staged": "^10.2.7",
    "path": "^0.12.7",
    "pug": "^3.0.0",
    "pug-lint": "^2.5.0",
    "stylelint": "13.5.0",
    "stylelint-config-htmlacademy": "0.1.x",
    "webpack": "^4.43.0",
    "webpack-stream": "^5.2.1"
  },
  "dependencies": {
    "@barba/prefetch": "^2.1.10",
    "core-js": "^3.6.5",
    "deep-extend": "^0.6.0",
    "gsap": "./gsap-bonus.tgz",
    "jquery": "^3.5.1",
    "katex": "^0.11.1",
    "mathjax": "3",
    "merge": "^1.2.1",
    "mkdirp": "^1.0.4",
    "normalize.less": "^1.0.0",
    "object-fit-images": "^3.2.4",
    "prismjs": "^1.21.0",
    "smoothscroll-polyfill": "^0.4.4",
    "stickyfilljs": "^2.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
